{
  "name": "financial",
  "version": "1.0.0",
  "engines": {
    "node": "10.9.0"
  },
  "description": "This application is made to track down your monthly expenses and incomes in the family and help you understand better where your money are going and how can you be more careful what you're spending money on.",
  "scripts": {
    "ci": "run-p --aggregate-output test lint:js",
    "build": "webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:hot": "cross-env NODE_ENV=hot webpack-dev-server --hot --inline",
    "build:watch": "webpack --watch",
    "start": "node ./bin/www",
    "heroku-prebuild": "npm install --only=dev",
    "heroku-postbuild": "npm run build:prod",
    "flow": "flow",
    "test": "jest --bail",
    "test:cov": "open build/coverage/index.html",
    "lint:js": "eslint server sources/mobile sources/common --quiet",
    "format:css": "prettier --write \"sources/**/*.pcss\"",
    "format:js": "prettier-eslint --write --single-quote --trailing-comma all",
    "format:all": "yarn run format:css; yarn run format:js \"server/**/*.js\" \"sources/mobile/**/*.js\" \"sources/common/**/*.js\"",
    "sql": "sequelize"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mircea-dinoiu/financial.git"
  },
  "author": "Mircea Dinoiu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mircea-dinoiu/financial/issues"
  },
  "homepage": "https://github.com/mircea-dinoiu/financial#readme",
  "devDependencies": {
    "@material-ui/core": "1.3.1",
    "@material-ui/icons": "1.1.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.6.0",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "compression-webpack-plugin": "2.0.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "eslint": "5.6.0",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.76.0",
    "husky": "0.14.3",
    "immutable": "3.8.2",
    "jest": "23.6.0",
    "lint-staged": "7.3.0",
    "material-ui": "0.20.2",
    "material-ui-pickers": "1.0.0-rc.11",
    "normalize.css": "8.0.0",
    "npm-run-all": "4.1.3",
    "postcss-loader": "2.1.5",
    "postcss-preset-env": "5.2.1",
    "prettier-eslint-cli": "4.7.1",
    "query-string": "6.1.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-event-listener": "0.6.3",
    "react-grid-system": "3.1.2",
    "react-hot-loader": "4.3.11",
    "react-redux": "5.0.7",
    "react-select": "1.2.1",
    "react-table": "6.8.6",
    "react-tap-event-plugin": "3.0.3",
    "react-tippy": "1.2.2",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "style-loader": "0.23.0",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.8",
    "webpack-manifest-plugin": "2.0.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "config": "1.25.1",
    "connect-session-sequelize": "5.2.2",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "debug": "2.6.3",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "hbs": "4.0.1",
    "lodash": "4.17.11",
    "md5": "2.2.1",
    "moment": "2.22.2",
    "morgan": "1.9.1",
    "mysql": "2.16.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "sequelize": "3.30.4",
    "sequelize-cli": "4.1.1",
    "serve-favicon": "2.5.0",
    "validator": "10.7.1",
    "xml2js": "0.4.19"
  },
  "lint-staged": {
    "*.js": [
      "yarn run format:js",
      "eslint --quiet",
      "git add"
    ],
    "*.pcss": [
      "yarn run format:css",
      "git add"
    ]
  }
}
