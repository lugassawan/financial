{
  "name": "financial",
  "version": "1.0.0",
  "engines": {
    "node": "10.4.1"
  },
  "description": "This application is made to track down your monthly expenses and incomes in the family and help you understand better where your money are going and how can you be more careful what you're spending money on.",
  "scripts": {
    "ci": "run-p --aggregate-output test lint:js",
    "build": "webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:hot": "cross-env NODE_ENV=hot webpack-dev-server --hot --inline",
    "build:watch": "webpack --watch",
    "start": "node ./bin/www",
    "heroku-prebuild": "npm install --only=dev",
    "heroku-postbuild": "npm run build:prod",
    "flow": "flow",
    "test": "jest --bail --config package.json",
    "lint:js": "eslint server sources/mobile sources/common --quiet",
    "format": "prettier-eslint --write --single-quote",
    "format:js": "yarn run format server/**/*.js sources/mobile/**/*.js sources/common/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mircea-dinoiu/financial.git"
  },
  "author": "Mircea Dinoiu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mircea-dinoiu/financial/issues"
  },
  "homepage": "https://github.com/mircea-dinoiu/financial#readme",
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-polyfill": "6.20.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.16.0",
    "compression-webpack-plugin": "0.4.0",
    "cross-env": "4.0.0",
    "css-loader": "0.28.1",
    "deep-extend": "0.4.1",
    "eslint": "5.0.1",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-react": "7.10.0",
    "flow-bin": "0.55.0",
    "husky": "0.14.3",
    "immutable": "3.8.1",
    "jest": "22.4.3",
    "lint-staged": "7.2.0",
    "material-ui": "0.20.1",
    "material-ui-icons": "1.0.0-alpha.3",
    "normalize.css": "7.0.0",
    "npm-run-all": "4.1.3",
    "postcss-loader": "2.1.5",
    "postcss-preset-env": "5.2.1",
    "prettier-eslint-cli": "4.7.1",
    "query-string": "6.0.0",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-event-listener": "0.5.3",
    "react-grid-system": "3.1.2",
    "react-hot-loader": "4.0.1",
    "react-redux": "5.0.6",
    "react-table": "6.8.6",
    "react-tap-event-plugin": "3.0.2",
    "react-tippy": "1.2.2",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "style-loader": "0.17.0",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.1",
    "whatwg-fetch": "2.0.3"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.17.1",
    "chalk": "1.1.3",
    "config": "1.25.1",
    "connect-session-sequelize": "4.1.0",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "debug": "2.6.3",
    "express": "4.15.2",
    "express-session": "1.15.2",
    "hbs": "4.0.1",
    "lodash": "4.17.3",
    "md5": "2.2.1",
    "moment": "2.22.2",
    "morgan": "1.8.1",
    "mysql": "2.13.0",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "sequelize": "3.30.4",
    "sequelize-cli": "4.0.0",
    "serve-favicon": "2.4.2",
    "validator": "7.0.0",
    "xml2js": "0.4.17"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "package.json",
      "sources/desktop/ext"
    ],
    "modulePaths": [
      "<rootDir>/",
      "<rootDir>/sources/"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn run format",
      "eslint --quiet",
      "git add"
    ]
  }
}
